alVisible} onOk={handleOk}>\n            {children}\n         </Modal>\n         </>\n        )\n}\nexport default RatingModal;","/home/sarzy/Downloads/ecommerce/client/src/function/ratings.js",[],"/home/sarzy/Downloads/ecommerce/client/src/components/Category/CategoryList.js",[],"/home/sarzy/Downloads/ecommerce/client/src/Pages/Category/CategoryHome.js",["317","318"],"import React,{useEffect,useState} from 'react';\nimport {getCategory} from '../../function/category'\nimport {Link} from 'react-router-dom'\nimport ProductCard from '../../components/cards/ProductCard'\n\nconst CategoryHome = ({match}) => {\n    const {slug} = match.params\n    const [category,setCategory] = useState({})\n    const [product,setProduct] = useState([])\n    const [loading,setLoading] = useState(false)\n\n    useEffect(()=>{\n        setLoading(true)\n        getCategory(slug).then((res)=> {\n            console.log(res.data)\n            setLoading(false)\n            setProduct(res.data.products)\n            setCategory(res.data.category)\n        })\n    },[])\n\n    return (\n        <div>\n            <div className=\"container-fluid\">\n                <div className=\"row\">\n                    <div className=\"col\">\n                    {loading ? (\n                <h4 className=\"text-center p-3 mb-3 mt-3 display-4 jumbotron\">\n                    Loading...\n                </h4>\n            ) : (\n                <h4 className=\"text-center p-3 mb-3 mt-3 display-4 jumbotron\">\n                    {product.length} Products were found in \"{category.name}\" Category\n                </h4>\n            ) }\n                    </div>\n                </div>\n\n                <div className=\"row\">\n                    {product.map(p => (\n                        <div className=\"col-md-4\" key={p._id}>\n                            <ProductCard product={p} />\n                            </div>\n                    ))}\n                </div>\n            </div>\n        