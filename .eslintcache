id duplicate\r\n            let unique = _.uniqWith(cart,_.isEqual)\r\n\r\n            //save to localstorage\r\n\r\n            localStorage.setItem('cart',JSON.stringify(unique))\r\n            setTooltip(\"Added\")\r\n            dispatch({\r\n                type:\"ADD_TO_CART\",\r\n                payload:unique\r\n            })\r\n\r\n            dispatch({\r\n                type:\"SET_VISIBLE\",\r\n                payload:true\r\n            })\r\n\r\n        }\r\n    }\r\n\r\n    return(\r\n        <>\r\n            {product && ratings && ratings.length > 0\r\n          ? showAverage(product)\r\n          : (<div className=\"text-center pt-3 pb-3\">No rating yet </div>)}\r\n\r\n\r\n\r\n        <Card cover={\r\n            <img src={images && images.length ? images[0].url : \"\"}\r\n            style={{height:\"150px\",objectFit:\"cover\"}}\r\n            className=\"p-1\" />\r\n        }\r\n        actions={[\r\n            <Link to={`/product/${slug}`}>\r\n                <EyeOutlined className=\"text-warning\"/> <br /> View Product\r\n            </Link>,\r\n            <Tooltip title={tooltip}>\r\n                <a onClick={handleAddToCart} > <ShoppingCartOutlined className=\"text-danger\"/> <br /> Add to cart </a>\r\n            </Tooltip> ,\r\n        ]}>\r\n            <Meta title={`${title} - $${price}`} description={`${description && description.substring(0,60)}...`} />\r\n\r\n        </Card>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default ProductCard;","/home/sarzy/Downloads/ecommerce/client/src/Pages/product.js",["302","303"],"import React,{useEffect,useState} from 'react'\nimport {getProduct,productStar} from '../function/product'\nimport SingleProduct from '../components/cards/SingleProduct'\nimport {toast} from 'react-toastify'\nimport {useSelector} from 'react-redux'\nimport {showAverage} from '../function/ratings'\nimport {getRelatedProduct} from '../function/product'\nimport ProductCard from '../components/cards/ProductCard'\n\nconst Pr